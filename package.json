{
  "name": "koishi-plugin-meeting-record",
  "version": "1.0.0",
  "description": "A koishi(https://github.com/koishijs/koishi) plugin to record specialized chat messages(Mainly to record meetings).",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run build:live",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts --exec 'ts-node' src/index.ts",
    "prettier:base": "prettier --parser typescript --single-quote",
    "prettier:check": "npm run prettier:base -- --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "npm run prettier:base -- --write \"src/**/*.{ts,tsx}\"",
    "precommit": "npm run prettier:write"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ConstasJ/meeting-record.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.10.9",
    "chai": "^4.3.4",
    "husky": "^7.0.2",
    "koishi": "^3.14.2",
    "koishi-adapter-onebot": "^3.1.0",
    "koishi-plugin-mongo": "^2.5.2",
    "koishi-plugin-mysql": "^3.6.2",
    "koishi-test-utils": "^7.0.2",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.13",
    "prettier": "^2.4.1",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4"
  }
}
